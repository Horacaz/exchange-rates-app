{
    "env": {
        "browser": true,
        "es2021": true,
        "jest/globals": true,
        "cypress/globals": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "ignorePatterns": ["*.json"],
    "rules": {
        "import/extensions": ["error", "always"]
    },
        "plugins": ["jest", "cypress"]
} 
